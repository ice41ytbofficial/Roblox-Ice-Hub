if game.PlaceId == 3956818381 then

local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local e = library:CreateWindow("Credits Window") 

local s = library:CreateWindow("Main Features")

local m = s:CreateFolder("Main")

local c = e:CreateFolder("Credits") 

local h = library:CreateWindow("Farm Features")

local z = h:CreateFolder("Auto Farm")



c:Label("Made By ice41ytbofficial#9999 using Wally V3 Remake",{
    TextSize = 10; 
    TextColor = Color3.fromRGB(255,255,255); 
    BgColor = Color3.fromRGB(69,69,69); 
    
}) 
m:Slider("WalkSpeed",{
    min = 0.1; -- min value of the slider
    max = 500; -- max value of the slider
    precise = true; -- max 2 decimals
},function(value)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = (value)
end)
c:Label("Have Fun Using Ninja Legends Gui :D",{
    TextSize = 10; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
}) 
m:Slider("JumpPower",{
    min = 1; -- min value of the slider
    max = 700; -- max value of the slider
    precise = true; -- max 2 decimals
},function(value)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = (value)
end)
m:Button("Infinite Jump",function()
    InfiniteJumpEnabled = true
game:GetService("UserInputService").JumpRequest:connect(function()
	if InfiniteJumpEnabled then
		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
	end
end)
end)
z:Button("Auto Swing",function()
    _G.swing = true
while _G.swing do
wait()
game.Players.LocalPlayer.ninjaEvent:FireServer("swingKatana")
end
end)
z:Button("Auto Sell",function()
    _G.sell = true
while _G.sell do
wait()
game.workspace["sellAreaCircle"].circleInner.CFrame = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame
end
end)
z:Button("Auto Buy Swords",function()
    _G.sword = true
while _G.sword do
wait()
--This script was generated by Hydroxide
local oh1 = "buyAllSwords"
local oh2 = {"Ground", "Astral Island", "Space Island"}
for i = 1, #oh2 do
    game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(oh1, oh2[i])
end
end
end)
z:Button("Farm Boss",function()
    _G.boss = true
while _G.boss do
wait()

if game:GetService("Workspace").bossFolder:FindFirstChild("RobotBoss"):FindFirstChild("HumanoidRootPart") then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.bossFolder.RobotBoss.HumanoidRootPart.CFrame
            end
end
end)
z:Button("Auto Buy Belts",function()
    _G.sword = true
    while _G.sword do
    wait()
    --This script was generated by Hydroxide
    local oh1 = "buyAllBelts"
    local oh2 = {"Ground", "Astral Island", "Space Island"}
    for i = 1, #oh2 do
        game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(oh1, oh2[i])
    end
    end
end)
z:Button("Farm Coins",function()
    _G.TpToCoins = true
while _G.TpToCoins do
    wait(0.1)
    for i, v in pairs(game.Workspace.spawnedCoins.Valley:GetChildren()) do
    wait(0.1)
        if  _G.TpToCoins then
           game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
        end
    end
end
end)
z:Button("Auto Upgrade Skills",function()
    _G.sword = true
while _G.sword do
wait()
--This script was generated by Hydroxide
local oh1 = "buyAllSkills"
local oh2 = {"Ground", "Astral Island", "Space Island"}
for i = 1, #oh2 do
    game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(oh1, oh2[i])
end
end
end)
m:Button("Infinite Health",function()
    Workspace.LocalPlayer.Humanoid.Health = 100000000000000000000
end)
m:Label("More Features Coming Soon!",{
    TextSize = 11; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
}) 
c:DestroyGui()
end





